<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="./images/logo-mercado-liebre.jpg" />

	<title> Mercado Liebre Argentina</title>
			<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/app.css">
</head>

<body>
	<!-- Header -->
	<header class="main-header">
		<%- include('./partials/headerLogueado.ejs', {user: usuario})%>
	</header>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Editar mi perfil</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<form action="/baba" method="get">
				<ul>
					<li>
						<label for="usuario">Usuario:</label>
						<input type="email" id="mail" name="usuario">
					</li>
					<li>
						<label for="contrasena">Contrase√±a:</label>
						<input type="password" id="contrasena" name="contrasena">
					</li>
					<li>
						<label for="fecha">fecha de nacimiento:</label>
						<input type="date" id="fecha" name="fecha">
					</li>
					<li>
						<label for="dni">DNI:</label>
						<input type="number" id="dni" name="dni">
					</li>
					<li>
						<label for="ftoPerfil">Foto de perfil:</label>
						<input type="text" id="ftoPerfil" name="ftoPerfil">
					</li>
					<button type="submit">Enviar</button>
				</ul>
			</form>
		</div>
	</div>


	<!-- footer -->
	<footer class="main-footer">
		<%- include('./partials/footer.ejs')%>
	</footer>
	<!-- /footer -->
</body>

</html>